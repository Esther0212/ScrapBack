{
  "expo": {
    "name": "ScrapBack",
    "slug": "ScrapBack",
    "scheme": "com.scrapback.app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/scrapback-icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,

    "splash": {
      "image": "./src/assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },

    "ios": {
      "supportsTablet": true
    },

    "android": {
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDAd_vfevQT0mQAnOV2ZbRBcGpLMMdVnXg"
        }
      },

      "adaptiveIcon": {
        "foregroundImage": "./src/assets/app-icon.png",
        "backgroundColor": "#F0F1C5"
      },

      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],

      "edgeToEdgeEnabled": true,
      "package": "com.scrapback.app"
    },

    "web": {
      "favicon": "./src/assets/favicon.png",
      "bundler": "metro"
    },

    "plugins": [
      "expo-router",
      "expo-font"
    ],

    "extra": {
      "router": {},
      "eas": {
        "projectId": "82e9e238-1aff-47d6-8778-6960b258804e"
      }
    },

    "owner": "missel012"
  }
}
