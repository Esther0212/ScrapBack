{
  "expo": {
    "name": "ScrapBack",
    "slug": "ScrapBack",
    "scheme": "com.scrapback.app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/app-logo.png",
    "userInterfaceStyle": "light",

    "splash": {
      "image": "./src/assets/app-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },

    "ios": {
      "supportsTablet": true
    },

    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/logo.png",
        "backgroundColor": "#F0F1C5"
      },
      "permissions": ["ACCESS_FINE_LOCATION", "ACCESS_COARSE_LOCATION"],
      "edgeToEdgeEnabled": true,
      "package": "com.missel012.ScrapBack",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDw7Ck3cTEICh-5MOQpE_lLue6bFJkHsoc"
        }
      }
    },

    "web": {
      "favicon": "./src/assets/favicon.png",
      "bundler": "metro"
    },

    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 34,
            "minSdkVersion": 24
          }
        }
      ],
      "expo-font",
      "expo-notifications"
    ],

    "extra": {
      "eas": {
        "projectId": "82e9e238-1aff-47d6-8778-6960b258804e"
      }
    },

    "owner": "missel012",
    "newArchEnabled": false,
    "updates": {
      "fallbackToCacheTimeout": 0
    }
  }
}
